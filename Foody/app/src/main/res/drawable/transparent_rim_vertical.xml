<?xml version="1.0" encoding="UTF-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" >

    <item android:width="5dp">
        <shape android:shape="rectangle" >
            <gradient
                android:angle="180"
                android:centerColor="@color/transparent"
                android:endColor="@color/dark_cyan"
                android:startColor="@color/transparent"
                android:type="linear" />
        </shape>
    </item>

    <item android:width="5dp" android:gravity="right">
        <shape android:shape="rectangle" >
            <gradient
                android:angle="0"
                android:centerColor="@color/transparent"
                android:endColor="@color/dark_cyan"
                android:startColor="@color/transparent"
                android:type="linear" />
        </shape>
    </item>
